Product product2 = new Product(111, "Sims");
product2.print();
 ID 111: Sims stock level: 0
product2.decreaseQuantity(10);
product2.print();
 ID 111: Sims stock level: 0
product2.print();
 ID 111: Sims stock level: 0
product2.increaseQuantity(10);
product2.print();
 ID 111: Sims stock level: 10
Product product3 = new Product(112, "Rainbow Six Siege");
